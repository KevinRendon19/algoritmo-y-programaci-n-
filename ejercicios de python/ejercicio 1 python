def promedio_edades():
    # Entradas
    edad1 = int(input("Ingrese edad 1: "))
    edad2 = int(input("Ingrese edad 2: "))
    edad3 = int(input("Ingrese edad 3: "))
    
    # Caja negra (cálculo del promedio)
    promedio = (edad1 + edad2 + edad3) / 3
    
    # Salida
    print("El promedio es:", promedio)

# Llamar a la función
promedio_edades()